<button class="show-more-button" onclick="showMoreEpisodes()">Lihat semua episode</button>

<script>
function showMoreEpisodes() {
  var hiddenEpisodes = document.getElementById("hiddenEpisodes");
  var button = document.querySelector(".show-more-button");
  if (hiddenEpisodes.style.display === "none") {
    hiddenEpisodes.style.display = "block";
    button.textContent = "Sembunyikan episode";
  } else {
    hiddenEpisodes.style.display = "none";
    button.textContent = "Lihat semua episode";
  }
}
</script>

